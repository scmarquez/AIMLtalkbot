	<pattern> CUANTOS HAY </pattern>
	<template>		
		<think>
			<set var="npersonas">
				<srai> COUNT <get name="personas"> </get> </srai>
			</set>
		</think>
		<condition var="npersonas">
			<li value="1">
				HE GANADO EL PERSONAJE ES <srai> TOP <get name="personas"/> </srai>
			</li>
			<li value="0">
				ERES UN TRAMPOSO
			</li>
			<li>
				<srai> HACER PREGUNTA DE VERDAD </srai>
			</li>
		</condition>
	</template>
</category>


<category>
	<pattern> TODOS IGUALES EN * </pattern>
	<template>
		<think>
		<set var="tipo">
			<star/>
		</set>
		
		<!--ASIGNA EL TIPO DE DATO QUE TIENE QUE COMPARAR-->
		<condition var="tipo">
			<li value="preguntas_ojos">
				<set var="atr">
					ojos
				</set>
			</li>
			<li value="preguntas_piel">
				<set var="atr">
					piel
				</set>
			</li>
			<li value="preguntas_pelo">
				<set var="atr">
					pelo
				</set>
			</li>
			<li value="preguntas_gafas">
				<set var="atr">
					gafas
				</set>
			</li>
			<li value="preguntas_barba">
				<set var="atr">
					barba
				</set>
			</li>
			<li value="preguntas_bigote">
				<set var="atr">
					bigote
				</set>
			</li>
			<li value="preguntas_sexo">
				<set var="atr">
					sexo
				</set>
			</li>
		</condition>

		<!-- Ponemos el primer atributo del primer personaje en una variable a y luego borramos a ese personaje. Comparamos cada atributo con el primero
		y si encontramos uno distinto devolvemos un no. En otro caso devolvemos si -->
		<set var="retorno">
			si
		</set>
		<set var="persona1">
			<srai> TOP <get name="personas"/> </srai>
		</set> 
		<set var="primero">
			<map><name><get var="atr"/></name> <get var="persona1"/></map>
		</set>
		la variable primero vale <get var="primero"/> y atr vale <get var="atr"/>
		<set var="item">
			<srai> ITERATE <get name="personas"/></srai>
		</set>
			
			
			<condition var="item">
				<li value="end"></li>
				<li> <get var="item"/>
					dentro del bucle
					<set var="atributo">
						<map><name><get var="atr"/></name> <get var="item"/> </map>
					</set>
					Siendo item <get var="item"/>
					La variable atributo vale <get var="atributo"/>
					<!--Ahora es necesario saber si el atributo cumple o no-->
					antes de comparar uno a uno
					<condition var="atributo">
						<li><value><get var="primero"/></value>
							vale lo mismo que el primero.
						</li>
						<li>
							<set var="retorno">
								no
							</set>
						</li>
					</condition>

				
						<set var="item">
							<srai>NEXT</srai>
						</set>
					
					<loop/>
				</li>
			</condition>
		</think>

		<get var="retorno"/>
	</template>
</category>

