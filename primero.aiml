-->
<category>
	<pattern>BORRA OJOS OSCUROS</pattern>
		<template>
			<think>
				<set var="item">
					<srai> ITERATE <get name="personas"/></srai>
				</set>
			</think>
			<think>
			<condition var="item">
				<li value="end"></li>
				<li> <get var="item"/>
					dentro del bucle
					
					<set var="atributo">
						<map><name>ojos</name> <get var="item"/> </map>
					</set>
					Siendo item <get var="item"/>
					La variable atributo vale <get var="atributo"/>
					<!--Ahora es necesario saber si el atributo cumple o no-->
					<condition var="atributo">
						<li value="claros">
							Este si los tiene claros.
						</li>
						<li>
							<srai>borra a <get var="item"/> </srai>
						</li>
					</condition>

					<think>
						<set var="item">
							<srai>NEXT</srai>
						</set>
					</think>
					<loop/>
				</li>
			</condition>
		</think>
		</template>
	</category>	

	<category>
		<pattern> BORRA OJOS CLAROS </pattern>
		<template>
			<think>
				<set var="item">
					<srai> ITERATE <get name="personas"/></srai>
