			<think>
			inicia preguntas 2
			<set var="preguntas2">
				<get name="preguntas"/>
			</set>
			inicia item
			<set var="item">
				<srai>ITERATE <get var="preguntas2"/></srai>
			</set>
			hace el iterate
			<condition var="item">
				<li value ="end"></li>
				<li> <get var="item"/>
					entra en comprueba pregunta donde item vale <get var="item"/>
					<set var="tipo">
						<srai> comprueba a pregunta <get var="item"/></srai>
					</set>
					sale de comprueba pregunta
					
					<set var="todosiguales">
						<srai>  todos iguales en <get var="tipo"/> </srai>
					</set>
					la variable todos iguales vale <get var="todosiguales"/>

					<condition var="todosiguales">
					<li value="si">
						entra en el borrado
						
						<srai> BORRA A PREGUNTA <get var="item"/></srai>
					
					</li>
					<li></li>
					</condition>
				
					actualiza la variable
					<set var="preguntas2">
						<srai>REMAIN <get var="preguntas2"/></srai>
					</set>
					<set var="item"> 
						<srai>TOP <get var="preguntas2"/></srai>
					</set>
					<loop/>
				</li>
			</condition>
		</think>
		</template>
	</category>

	<category>
		<pattern> comprueba a pregunta * </pattern>
		<template> 
			<think>
			<set var="pregunta">
				<star/>
			</set>
				
				
				<!--ANTES DEBE SABER SI LA PREGUNTA QUE VA HA HACER TIENE SENTIDO HACERLA O NO--> 
				<!--PARA ELLO MIRA EN QUE CATEGORIA ESTÁ LA PREGUNTA ACTUAL ITERANDO SOBRE TODAS LAS CATEGORIAS.-->
				
				<set var="item">
					<srai> ITERATE <get name="tipo_preguntas"/></srai>
				</set>
				
				<condition var="item">
				<li value="end"></li>
				<li> <get var="item"/>
					<set var="posicion">
						<!--Busca la pregunta en ese tipo de pregunta-->
						<srai>BUSCA A PREGUNTA <get var="pregunta"/> en <get var="item"/></srai>
					</set>
						<condition var="posicion">
							<li value="0">  entra en el 0 </li>
							<li> 
								<set var="tipo">
									<get var="item"/>
								</set>
							</li>
						</condition>
							<set var="item"> 
								<srai>NEXT</srai>
							</set>
					<loop/>
				</li>
				</condition>
				la pregunta ha sido encontrada en <get var="tipo"/>
			</think>
			<get var="tipo"/>
			</template>
		</category>

<!--Hacer una pregunta -->
<category>
	<pattern>HACER PREGUNTA DE VERDAD</pattern>
	<template>
				
				<srai> COMPRUEBA PREGUNTAS </srai>
				<set var="pregunta">
				<srai> SELECTITEM 1 IN <get name="preguntas"/> </srai>
				</set>
				
				<srai>LISTA PREGUNTAS</srai>
				<!-- abrir el map con la clave de la pregunta para mostrarla en pantalla-->
				<map><name>preguntas</name> <get var="pregunta"/> </map>
				<think>
				<!-- borrar la pregunta del conjunto de preguntas de su correspondiente tipo, ya que se escogerán de ahi.-->
				Antes de borrar la pregunta
				<set name="preguntas">
					<srai> REMOVEITEM 1 IN <get name="preguntas"/></srai>
				</set>
				antes del listado
				<srai>LISTA PREGUNTAS</srai>
				<!--cambiar el topic del robot para que la respuesta que demos a continuación descarte personajes y preguntas-->
				sale del listado
				<set><name>topic</name> <get var="pregunta"/></set>
				</think>
	</template>
</category>



