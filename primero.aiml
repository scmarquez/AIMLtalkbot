<!-- comprobar el numero de personas que quedan -->
<category>
	<pattern> BORRA A PREGUNTA * </pattern>
	<template>
		<think>
		<set var="pos">
			<srai>FINDITEM <star/> IN <get name="preguntas"/></srai>
		</set>
		<!--Borra al personaje-->
		<set name="preguntas">
			<srai> REMOVEITEM <get var="pos"/> IN <get name="preguntas"/> </srai>
		</set>	
		</think>

	</template>
</category>

